  "tenant" varchar COLLATE "pg_catalog"."default" NOT NULL,
  "platform" varchar COLLATE "pg_catalog"."default",
  "createTime" timestamp(6) NOT NULL DEFAULT CURRENT_TIMESTAMP,
  "updateTime" timestamp(6) NOT NULL DEFAULT CURRENT_TIMESTAMP,
  "createBy" varchar COLLATE "pg_catalog"."default",
  "updateBy" varchar COLLATE "pg_catalog"."default",
  CONSTRAINT "ctint_report_permission_pkey" PRIMARY KEY ("reportPermissionId")
);
