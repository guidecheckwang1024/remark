  "reportMasterId" COLLATE "pg_catalog"."default" "pg_catalog"."text_ops" ASC NULLS LAST
);






CREATE TABLE "public"."ctint_job_details_report" (
  "jobDetailsId" uuid NOT NULL DEFAULT gen_random_uuid(),
  "jobId" uuid NOT NULL,
  "reportMasterId" varchar(40) COLLATE "pg_catalog"."default",
  "workerName" varchar(40) COLLATE "pg_catalog"."default",
  "parameters" text COLLATE "pg_catalog"."default",
  "status" varchar(20) COLLATE "pg_catalog"."default",
  "result" text COLLATE "pg_catalog"."default",
  "createTime" timestamp(6) DEFAULT CURRENT_TIMESTAMP,
  "updateTime" timestamp(6),
  "createBy" varchar(40) COLLATE "pg_catalog"."default",
  "updateBy" varchar(40) COLLATE "pg_catalog"."default",
  "reportRecordId" varchar(40) COLLATE "pg_catalog"."default",
  "implementUrl" varchar(255) COLLATE "pg_catalog"."default",
  CONSTRAINT "ctint_job_details_report_pkey" PRIMARY KEY ("jobDetailsId")
)
;
CREATE INDEX "CTINT_JOB_DETAILS_REPORT_IDX_1" ON "public"."ctint_job_details_report" USING btree (
  "jobDetailsId" "pg_catalog"."uuid_ops" ASC NULLS LAST
);
CREATE INDEX "CTINT_JOB_DETAILS_REPORT_IDX_2" ON "public"."ctint_job_details_report" USING btree (
  "jobId" "pg_catalog"."uuid_ops" ASC NULLS LAST
);

CREATE TABLE "public"."ctint_job_details_stt" (
  "jobDetailsId" uuid NOT NULL DEFAULT gen_random_uuid(),
  "jobId" uuid NOT NULL,
  "mediaUri" varchar(100) COLLATE "pg_catalog"."default" NOT NULL,
  "mediaSource" varchar(40) COLLATE "pg_catalog"."default" NOT NULL,
  "duration" int4 NOT NULL,
  "transcript" text COLLATE "pg_catalog"."default",
  "workerName" varchar(40) COLLATE "pg_catalog"."default",
  "createTime" timestamp(6) DEFAULT CURRENT_TIMESTAMP,
  "createBy" varchar(20) COLLATE "pg_catalog"."default" DEFAULT 'SYSTEM'::character varying,
  "updateTime" timestamp(6),
  "updateBy" varchar(20) COLLATE "pg_catalog"."default" DEFAULT 'SYSTEM'::character varying,
  "parameters" text COLLATE "pg_catalog"."default",
  "language" varchar(20) COLLATE "pg_catalog"."default",
  CONSTRAINT "ctint_job_details_stt_pkey" PRIMARY KEY ("jobDetailsId"),
  CONSTRAINT "fk_ctint_job_details_stt_ctint_job" FOREIGN KEY ("jobId") REFERENCES "public"."ctint_job" ("jobId") ON DELETE NO ACTION ON UPDATE NO ACTION
)
;




CREATE TABLE "public"."ctint_job_details_auto_qm" (
  "jobDetailsId" uuid NOT NULL DEFAULT gen_random_uuid(),
  "jobId" uuid NOT NULL,
  "result" text COLLATE "pg_catalog"."default",
  "workerName" varchar(40) COLLATE "pg_catalog"."default",
  "createTime" timestamp(6) DEFAULT CURRENT_TIMESTAMP,
  "createBy" varchar(20) COLLATE "pg_catalog"."default" DEFAULT 'SYSTEM'::character varying,
  "updateTime" timestamp(6),
  "updateBy" varchar(40) COLLATE "pg_catalog"."default" DEFAULT 'SYSTEM'::character varying,
  "parameters" text COLLATE "pg_catalog"."default",
  "recordingId" varchar(40) COLLATE "pg_catalog"."default",
  "status" varchar(20) COLLATE "pg_catalog"."default",
  CONSTRAINT "ctint_job_details_nlp_pkey" PRIMARY KEY ("jobDetailsId"),
  CONSTRAINT "fk_ctint_job_details_nlp_ctint_job" FOREIGN KEY ("jobId") REFERENCES "public"."ctint_job" ("jobId") ON DELETE NO ACTION ON UPDATE NO ACTION
);
CREATE INDEX "CTINT_JOB_DETAIL_AUTO_QM_IDX_1" ON "public"."ctint_job_details_auto_qm" USING btree (
  "jobId" "pg_catalog"."uuid_ops" ASC NULLS LAST
);
CREATE INDEX "CTINT_JOB_DETAIL_AUTO_QM_IDX_2" ON "public"."ctint_job_details_auto_qm" USING btree (
  "recordingId" COLLATE "pg_catalog"."default" "pg_catalog"."text_ops" ASC NULLS LAST
);



CREATE TABLE "public"."ctint_job_details_ocr" (
  "jobDetailsId" uuid NOT NULL DEFAULT gen_random_uuid(),
  "jobId" uuid NOT NULL,
  "imageUri" varchar(100) COLLATE "pg_catalog"."default" NOT NULL,
  "imageSource" varchar(40) COLLATE "pg_catalog"."default" NOT NULL,
  "parameters" text COLLATE "pg_catalog"."default",
  "workerName" varchar(40) COLLATE "pg_catalog"."default",
  "expiryTime" timestamp(6),
  "createTime" timestamp(6) DEFAULT CURRENT_TIMESTAMP,
  "createBy" varchar(20) COLLATE "pg_catalog"."default" DEFAULT 'SYSTEM'::character varying,
  "updateTime" timestamp(6),
  "updateBy" varchar(20) COLLATE "pg_catalog"."default" DEFAULT 'SYSTEM'::character varying,
  "outputImageUri" varchar(500) COLLATE "pg_catalog"."default",
  "isHkid" varchar(5) COLLATE "pg_catalog"."default",
  "isBankcard" varchar(5) COLLATE "pg_catalog"."default",
  "entities" text COLLATE "pg_catalog"."default",
  CONSTRAINT "ctint_job_details_ocr_pkey" PRIMARY KEY ("jobDetailsId"),
  CONSTRAINT "fk_ctint_job_details_ocr_ctint_job" FOREIGN KEY ("jobId") REFERENCES "public"."ctint_job" ("jobId") ON DELETE NO ACTION ON UPDATE NO ACTION
)
;


