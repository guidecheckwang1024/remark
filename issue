CREATE TABLE "public"."ctint_report_master" (
  "reportMasterId" varchar(40) COLLATE "pg_catalog"."default" NOT NULL DEFAULT gen_random_uuid(),
  "reportType" varchar COLLATE "pg_catalog"."default" NOT NULL,
  "reportName" varchar COLLATE "pg_catalog"."default" NOT NULL,
  "description" text COLLATE "pg_catalog"."default",
  "tenant" varchar COLLATE "pg_catalog"."default" NOT NULL,
  "platform" varchar COLLATE "pg_catalog"."default",
  "createTime" timestamp(6) NOT NULL DEFAULT CURRENT_TIMESTAMP,
  "updateTime" timestamp(6) DEFAULT CURRENT_TIMESTAMP,
  "createBy" varchar COLLATE "pg_catalog"."default",
  "updateBy" varchar COLLATE "pg_catalog"."default",
  "implementUrl" varchar COLLATE "pg_catalog"."default",
  CONSTRAINT "ctint_report_master_pkey" PRIMARY KEY ("reportMasterId")
)
;
CREATE INDEX "CTINT_REPORT_MASTER_IDX_1" ON "public"."ctint_report_master" USING btree (
  "reportMasterId" COLLATE "pg_catalog"."default" "pg_catalog"."text_ops" ASC NULLS LAST
);
CREATE INDEX "CTINT_REPORT_MASTER_IDX_2" ON "public"."ctint_report_master" USING btree (
  "reportType" COLLATE "pg_catalog"."default" "pg_catalog"."text_ops" ASC NULLS LAST
);









CREATE TABLE "public"."ctint_report_detail" (
  "reportDetailId" varchar(40) COLLATE "pg_catalog"."default" NOT NULL DEFAULT gen_random_uuid(),
  "reportMasterId" varchar(40) COLLATE "pg_catalog"."default" NOT NULL,
  "detailType" varchar COLLATE "pg_catalog"."default" NOT NULL,
  "detailSubType" varchar COLLATE "pg_catalog"."default",
  "detailKey" varchar COLLATE "pg_catalog"."default" NOT NULL,
  "detailValue" text COLLATE "pg_catalog"."default",
  "detailRemark" text COLLATE "pg_catalog"."default",
  "enable" bool DEFAULT true,
  "isRequired" bool DEFAULT false,
  "tenant" varchar COLLATE "pg_catalog"."default" NOT NULL,
  "platform" varchar COLLATE "pg_catalog"."default",
  "createTime" timestamp(6) NOT NULL DEFAULT CURRENT_TIMESTAMP,
  "updateTime" timestamp(6) DEFAULT CURRENT_TIMESTAMP,
  "createBy" varchar COLLATE "pg_catalog"."default",
  "updateBy" varchar COLLATE "pg_catalog"."default",
  "parentDetailId" varchar(40) COLLATE "pg_catalog"."default",
  "dependencyDetailId" varchar(40) COLLATE "pg_catalog"."default",
  "dependencyValue" text COLLATE "pg_catalog"."default",
  "sortOrder" int4,
  CONSTRAINT "ctint_report_detail_pkey" PRIMARY KEY ("reportDetailId"),
  CONSTRAINT "fk_report_master" FOREIGN KEY ("reportMasterId") REFERENCES "public"."ctint_report_master" ("reportMasterId") ON DELETE CASCADE ON UPDATE NO ACTION
)
;
CREATE INDEX "CTINT_REPORT_DETAIL_IDX_1" ON "public"."ctint_report_detail" USING btree (
  "reportDetailId" COLLATE "pg_catalog"."default" "pg_catalog"."text_ops" ASC NULLS LAST
);
CREATE INDEX "CTINT_REPORT_DETAIL_IDX_2" ON "public"."ctint_report_detail" USING btree (
  "reportMasterId" COLLATE "pg_catalog"."default" "pg_catalog"."text_ops" ASC NULLS LAST
);
CREATE INDEX "CTINT_REPORT_DETAIL_IDX_3" ON "public"."ctint_report_detail" USING btree (
  "parentDetailId" COLLATE "pg_catalog"."default" "pg_catalog"."text_ops" ASC NULLS LAST
);
CREATE INDEX "CTINT_REPORT_DETAIL_IDX_4" ON "public"."ctint_report_detail" USING btree (
  "dependencyDetailId" COLLATE "pg_catalog"."default" "pg_catalog"."text_ops" ASC NULLS LAST
);












CREATE TABLE "public"."ctint_report_record" (
  "reportRecordId" varchar(40) COLLATE "pg_catalog"."default" NOT NULL DEFAULT gen_random_uuid(),
  "reportMasterId" varchar(40) COLLATE "pg_catalog"."default" NOT NULL,
  "state" varchar COLLATE "pg_catalog"."default",
  "fileUrl" text COLLATE "pg_catalog"."default",
  "expiryDate" timestamp(6),
  "tenant" varchar COLLATE "pg_catalog"."default",
  "platform" varchar COLLATE "pg_catalog"."default",
  "createTime" timestamp(6) NOT NULL DEFAULT CURRENT_TIMESTAMP,
  "updateTime" timestamp(6) DEFAULT CURRENT_TIMESTAMP,
  "createBy" varchar COLLATE "pg_catalog"."default",
  "updateBy" varchar COLLATE "pg_catalog"."default",
  "jobId" varchar(40) COLLATE "pg_catalog"."default",
  CONSTRAINT "ctint_report_record_pkey" PRIMARY KEY ("reportRecordId")
)
;
CREATE INDEX "CTINT_REPORT_RECORD_IDX_1" ON "public"."ctint_report_record" USING btree (
  "reportRecordId" COLLATE "pg_catalog"."default" "pg_catalog"."text_ops" ASC NULLS LAST
);
CREATE INDEX "CTINT_REPORT_RECORD_IDX_2" ON "public"."ctint_report_record" USING btree (
  "reportMasterId" COLLATE "pg_catalog"."default" "pg_catalog"."text_ops" ASC NULLS LAST
);
CREATE INDEX "CTINT_REPORT_RECORD_IDX_3" ON "public"."ctint_report_record" USING btree (
  "jobId" COLLATE "pg_catalog"."default" "pg_catalog"."text_ops" ASC NULLS LAST
);
CREATE INDEX "CTINT_REPORT_RECORD_IDX_4" ON "public"."ctint_report_record" USING btree (
  "state" COLLATE "pg_catalog"."default" "pg_catalog"."text_ops" ASC NULLS LAST
);












CREATE TABLE "public"."ctint_report_permission" (
  "reportPermissionId" varchar COLLATE "pg_catalog"."default" NOT NULL DEFAULT gen_random_uuid(),
